    <vivo:educationalBackground rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;ul>&lt;li>Ph.D.				,	University of Missouri, 1972&lt;/li>&lt;li>M.S.,					University of Missouri,  1968	&lt;/li>&lt;li>B.S.,					NW Missouri State University,  1966	&lt;/li>&lt;/ul></vivo:educationalBackground>
